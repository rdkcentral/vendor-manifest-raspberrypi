<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <yocto version="kirkstone" />
    <remote fetch="https://github.com/rdkcentral" name="rdkcentral" review="https://github.com/rdkcentral"/>
    <remote fetch="https://code.rdkcentral.com/r/a/" name="cmf" review="https://code.rdkcentral.com/r/a/"/>
    <remote fetch="https://git.yoctoproject.org" name="oe" review="https://git.yoctoproject.org"/>

    <project groups="buildscripts" name="build-scripts" path="scripts" remote="rdkcentral" revision="refs/tags/1.0.1">
        <annotation name="MANIFEST_EXPORT_PATH" value="MANIFEST_PATH_SCRIPTS"/>
    </project>

    <!-- Common submanifest -->
    <submanifest name="common" project="vendor-manifest-raspberrypi" manifest-name="rdke-common.xml" path="rdke" remote="rdkcentral" revision="develop">
    </submanifest>

    <!-- Vendor layers submanifest -->
    <submanifest name="platform" project="vendor-manifest-raspberrypi" manifest-name="rdke-rpi-platform.xml" path="rdke/vendor" remote="rdkcentral" revision="develop">
    </submanifest>

    <!-- product layer submanifest -->
    <submanifest name="product" project="vendor-manifest-raspberrypi" manifest-name="rdke-rpi-product.xml" path="rdke/product" remote="rdkcentral" revision="develop">
    </submanifest>

    <!-- soc submanifest -->
    <submanifest name="rtk" project="collaboration/soc/realtek/vendor-manifest-rdke-realtek" manifest-name="rdke-rtk-vendor.xml" path="rdke" remote="cmf" revision="feature/RDKEVL-6715-POC">
    </submanifest>

    <submanifest name="bcm" project="collaboration/soc/broadcom/vendor-manifest-rdke-broadcom" manifest-name="rdke-bcm-vendor.xml" path="rdke" remote="cmf" revision="feature/RDKEVL-6715-POC">
    </submanifest>

